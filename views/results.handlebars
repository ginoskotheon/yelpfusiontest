<h1 class="page-header">Fusion Yelp</h1>
<div class="row">
  {{#each data}}
  <div class="col-md-3 text-center">
    <img src="{{this.image_url}}" alt="No Image Available">
    <h4>{{this.name}}</h4>
    <h4>{{this.location.city}}, {{this.location.state}}</h4>
    <h4>Rating: {{this.rating}}</h4>
  </div>
  {{/each}}
</div>
